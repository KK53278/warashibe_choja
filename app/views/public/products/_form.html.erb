<%= form_with model:product, url: public_products_path do |f| %>

  <div class="form-group">

    <h6>商品画像</h6>
      <%= f.file_field :images, multiple: true, accept: "image/*" %>
    <h6>商品名</h6>
      <%= f.text_field :product_name %>
    <h6>商品説明</h6>
      <%= f.text_area :caption %>
  </div>

  <div class="form-group">
    <%= f.submit '投稿' %>
  </div>
<% end %>